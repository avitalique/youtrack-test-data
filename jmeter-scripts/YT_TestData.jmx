            <stringProp name="script">import groovy.json.JsonOutput
import groovy.json.JsonSlurper

// Map requested IDs to actual ones
def mapIssueID = [:]
File file = new File(vars.get(&quot;datasetCreatedIssues&quot;))
file.withReader { reader -&gt;
	while ((line = reader.readLine()) != null) {
		def issue = new JsonSlurper().parseText(line)
		String requestedId = &quot;&quot; + issue.requestedProjectKey + &quot;-&quot; + issue.requestedIssueNumber
		String createdId = issue.issueIdReadable
		mapIssueID.put(requestedId, createdId)
     }
}

// Read issue links from created_issues.csv file
def issueLinks = []
file.withReader { reader -&gt;
	while ((line = reader.readLine()) != null) {
		def issue = new JsonSlurper().parseText(line)
		String requestedSourceId = &quot;&quot; + issue.requestedProjectKey + &quot;-&quot; + issue.requestedIssueNumber
		issue.requestedIssueLinks.each {
			String requestedTargetId = &quot;&quot; + issue.requestedProjectKey + &quot;-&quot; + it.targetIssueNumber
			issueLinks &lt;&lt; [
				&apos;sourceId&apos;: mapIssueID.get(requestedSourceId), 
				&apos;type&apos;: it.linkType,
				&apos;targetId&apos;: mapIssueID.get(requestedTargetId)
			]
		}
     }
}

// Write issue_links.csv
file = new File(vars.get(&quot;datasetCreatedLinks&quot;))
issueLinks.each {
	file.append(JsonOutput.toJson(it) + &quot;\n&quot;)
}
